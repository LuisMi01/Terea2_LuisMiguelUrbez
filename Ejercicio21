@startuml
class EntidadGeográficaA{
Código: 1 Texto
Nombre: 1 Texto
}
class Punto{
X: 1 Número
Y: 1 Número
Z: 1 Número
}

class Linea {}
class Area{}

Punto "2..*"--o "0..*" Linea 
Punto "3..*"--o "0..*" Area 

Punto --|> EntidadGeográficaA
Linea --|> EntidadGeográficaA
Area --|> EntidadGeográficaA
@enduml
